      }
    },
    "local-jwt": {
      "type": "library",
      "root": "libs/local-jwt",
      "entryFile": "index",
      "sourceRoot": "libs/local-jwt/src",
      "compilerOptions": {
        "tsConfigPath": "libs/local-jwt/tsconfig.lib.json"
      }
    },
    "local-pdf": {
