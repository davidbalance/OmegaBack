{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "plugins": [
      "@nestjs/swagger"
    ]
  },
  "projects": {
    "db-logger": {
      "type": "library",
      "root": "libs/db-logger",
      "entryFile": "index",
      "sourceRoot": "libs/db-logger/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db-logger/tsconfig.lib.json"
      }
    },
    "html-loader": {
      "type": "library",
      "root": "libs/html-loader",
      "entryFile": "index",
      "sourceRoot": "libs/html-loader/src",
      "compilerOptions": {
        "tsConfigPath": "libs/html-loader/tsconfig.lib.json"
      }
    },
    "local-email": {
      "type": "library",
      "root": "libs/local-email",
      "entryFile": "index",
      "sourceRoot": "libs/local-email/src",
      "compilerOptions": {
        "tsConfigPath": "libs/local-email/tsconfig.lib.json"
      }
    },
    "local-file": {
      "type": "library",
      "root": "libs/local-file",
      "entryFile": "index",
      "sourceRoot": "libs/local-file/src",
      "compilerOptions": {
